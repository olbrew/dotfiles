# Temp shortcuts
alias ini='find . -type f -name "*.ini" -print  -exec ../../src/engine {} \;'
alias l2p='vagrant ssh -- python3 /vagrant/src/manage.py runserver 0.0.0.0:8000'
alias antlr4='java -jar /usr/local/lib/antlr-4.5-complete.jar'
alias grun='java org.antlr.v4.runtime.misc.TestRig'

# Shell
alias j='jump'
alias m='mark'
alias c='clear'
alias q='clear && exit'
alias rm='trash'
alias log='tail /var/log/system.log'
alias vimrc='vim ~/.vimrc'
alias src='source ~/.zshrc'
alias c++='clang++ -std=c++14 -Wall -Wextra'

# Utilities
alias ip='curl https://icanhazip.com/'
alias t='todo.sh -d ~/.todo.cfg -A -c'
alias vit='vim ~/Documents/Personal/todo.txt'
alias vis='vim ~/Documents/Personal/schedule.md'
alias bru='brew update && brew upgrade && brew cleanup && brew cask cleanup && brew prune'
alias texu='tlmgr update --self --all --reinstall-forcibly-removed'
alias axel='axel -o ~/Downloads'
alias yt='youtube-dl -o "~/Downloads/%(title)s.%(ext)s"'
