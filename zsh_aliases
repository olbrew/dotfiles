# Temp shortcuts
alias ini='find . -type f -name "*.ini" -print  -exec ../../src/engine {} \;'
alias wkserver='vagrant ssh -- python /vagrant/src/manage.py runserver 0.0.0.0:8000'

# Shell 
alias j='jump'
alias m='mark'
alias c='clear'
alias q='clear && exit'
alias log='tail /var/log/system.log'
alias axel='axel -o ~/Downloads'
alias vimrc='vim ~/.vimrc'
alias src='source ~/.zshrc'
alias c++='clang++ -std=c++11 -Wall -Wextra'

# Utilities
alias ip='curl https://icanhazip.com/'
alias t='todo.sh -d ~/.todo.cfg -A -c'
alias vit='vim ~/Documents/Personal/todo.txt'
alias bru='brew update && brew upgrade && brew cleanup && brew cask cleanup'
alias texu='tlmgr update --self --all --reinstall-forcibly-removed'
